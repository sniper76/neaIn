<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="setUserCmpny">
    
    <delete id="deleteUserInfoAjax" parameterType="UserCmpnyBean">
		UPDATE CPES_USER
		SET USE_YN					= 'N'
		WHERE USER_SEQ 				= #{userSeq}
    </delete>     

    <delete id="deleteCmpnyInfoAjax" parameterType="UserCmpnyBean">
		UPDATE CPES_COMPNY
		SET DEL_YN					= 'Y'
		WHERE USER_SEQ 				= #{userSeq}
    </delete>           
</mapper>
